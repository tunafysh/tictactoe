datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Player {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  email     String   @unique
  password  String
  wins      Int
  losses    Int
}